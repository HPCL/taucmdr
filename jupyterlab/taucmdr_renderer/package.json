{
  "name": "taucmdr_renderer",
  "version": "0.1.0",
  "description": "TAU Commander MIME Renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^0.4.2",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "@types/three": "^0.84.35",
    "moremath": "^1.1.6",
    "three": "^0.88.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.58",
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "author": "ParaTools, Inc.",
  "license": "BSD-3-Clause"
}
